  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "unified",
    "rehype",
    "rehype-plugin",
    "plugin",
    "html",
    "hast",
    "markdown"
  ],
  "repository": "themosaad/rehype-external-img-size",
